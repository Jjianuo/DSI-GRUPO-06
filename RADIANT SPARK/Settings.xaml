    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.75*"/>
            <RowDefinition Height="0.5*"/>
            <RowDefinition Height="0.5*"/>
            <RowDefinition Height="0.75*"/>
            <RowDefinition Height="0.5*"/>
            <RowDefinition Height="0.75*"/>
            <RowDefinition Height="0.5*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>

        <Image Source="Assets/settingBG.png" HorizontalAlignment="Center" VerticalAlignment="Center"
               Stretch="UniformToFill" Grid.RowSpan="8" Grid.ColumnSpan="5"/>
        <Rectangle Fill="SteelBlue" Opacity="0.5" Grid.ColumnSpan="4"/>
        <StackPanel Orientation="Horizontal" Grid.ColumnSpan="2" Spacing="20">
            <Viewbox Grid.ColumnSpan="3" HorizontalAlignment="Left" Margin="15">
                <SymbolIcon Symbol="Setting" Foreground="FloralWhite" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
            </Viewbox>
            <Viewbox Grid.ColumnSpan="3" HorizontalAlignment="Left">
                <TextBlock x:Uid="TitleT_Settings" VerticalAlignment="Center" HorizontalAlignment="Left"/>
            </Viewbox>
        </StackPanel>
        <Viewbox  Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2" VerticalAlignment="Bottom">
            <TextBlock x:Uid="MusicVT_Settings" FontSize="60"/>
        </Viewbox>
        <Slider x:Name="MusicSlider" Style="{StaticResource ThickerSlider}" Grid.Row="3" Grid.Column="1" Grid.ColumnSpan="2" VerticalAlignment="Top" TickFrequency="20" TickPlacement="Inline" Orientation="Horizontal" 
            Maximum="100" Minimum="0" Value="50" ValueChanged="MusicSlider_ValueChanged" IsFocusEngagementEnabled="True"/>
        <Viewbox  Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="2" VerticalAlignment="Bottom">
            <TextBlock x:Uid="SoundVT_Settings" FontSize="60"/>
        </Viewbox>
        <Slider x:Name="SoundSlider" Style="{StaticResource ThickerSlider}" Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="2" VerticalAlignment="Top" TickFrequency="20" TickPlacement="Inline" Orientation="Horizontal" 
            Maximum="100" Minimum="0" Value="50" Foreground="White" ValueChanged="SoundSlider_ValueChanged"/>
        <Viewbox Grid.Row="6" Grid.Column="1" VerticalAlignment="Bottom">
            <TextBlock x:Uid="FullscreenT_Settings" FontSize="60" SelectionChanged="TextBlock_SelectionChanged"/>
        </Viewbox>
        <Viewbox Grid.Row="7" Grid.Column="1" VerticalAlignment="Top" Height="75">
            <CheckBox Style="{StaticResource CheckBoxStyle1}" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked" 
                      HorizontalAlignment="Center" Padding="0" MinWidth="0" BorderBrush="{x:Null}"/>
        </Viewbox>
        <Viewbox Grid.Row="6" Grid.Column="2" VerticalAlignment="Bottom">
            <TextBlock x:Uid="LanguageT_Settings" FontSize="60"/>
        </Viewbox>
        <Viewbox Grid.Row="7" Grid.Column="2" VerticalAlignment="Top" Height="75">
            <ComboBox x:Name="Language" Grid.Row="7" Grid.Column="1" SelectedItem="English" SelectionChanged="Language_SelectionChanged">
                <x:String>Spanish</x:String>
                <x:String>English</x:String>
            </ComboBox>
        </Viewbox>
        <Button HorizontalAlignment="Left" VerticalAlignment="Bottom"
                Width="60" Height="40" Background="Transparent" Grid.Row="7" Style="{StaticResource NavigationBackButtonNormalStyle}" 
                Foreground="White" ToolTipService.ToolTip="Back" Click="Back_click"/>
    </Grid>
